---
import { Card } from "./ProjectCard.tsx";
import type { Post } from "../pages/index.astro"

export interface Props {
    title: string;
    posts: any[];
}

const { title, posts } = Astro.props;

---

<h2>{title}</h2>
<ul>
    {posts.map((i) => {
    const post = i as Post;
        return (
            <Card client:load {...post}/>
        );
    })}
</ul>